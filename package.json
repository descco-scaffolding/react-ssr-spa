  "name": "appweb",
  "version": "0.0.0",
  "description": "Web App",
  "scripts": {
    "start": "node dist/server.min.js",
    "start_dev": "./node_modules/.bin/cross-env BABEL_ENV=development nodemon source/server/index.js",
    "start:development": "npm run clean && npm run build:static && npm run build:fonts && npm run start_dev",
    "clean": "./node_modules/.bin/rimraf 'dist'",
    "prebuild": "npm run clean",
    "build": "./node_modules/.bin/cross-env NODE_ENV=production BABEL_ENV=production run-p build:*",
    "build:static": "./node_modules/.bin/cross-env BABEL_ENV=static ./node_modules/.bin/babel source/static --out-dir dist --copy-files",
    "build:fonts": "npm run build_scadia_regular && npm run build_scadia_bold",
    "build:server": "./node_modules/.bin/cross-env BABEL_ENV=server ./node_modules/.bin/webpack --config ./internals/webpack/server.js",
    "build:client": "./node_modules/.bin/cross-env BABEL_ENV=client ./node_modules/.bin/webpack --config ./internals/webpack/client.js",
    "build_analyze": "./node_modules/.bin/cross-env ANALYZE_MODE=enabled run-p build:*",
    "lint": "run-p lint:*",
    "lint:sass": "./node_modules/.bin/sass-lint 'source/shared/**/*.scss' --verbose --no-exit",
    "lint:js": "./node_modules/.bin/eslint source",
    "test": "./node_modules/.bin/cross-env BABEL_ENV=testing ./node_modules/.bin/jest",
    "release": "./node_modules/.bin/release-it",
    "heroku-postbuild": "npm run build"
  },
